<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>HttpClientServlet1</servlet-name>
		<servlet-class>com.zhuc.my.httpclient.t1.TestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HttpClientServlet1</servlet-name>
		<url-pattern>/servlet/HttpClientServlet1</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MultipartServlet</servlet-name>
		<servlet-class>com.zhuc.my.httpclient.t2.MultipartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MultipartServlet</servlet-name>
		<url-pattern>/servlet/MultipartServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LongConnectServlet</servlet-name>
		<servlet-class>com.zhuc.my.before_websocket.t1.LongConnectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LongConnectServlet</servlet-name>
		<url-pattern>/servlet/longConnectServlet</url-pattern>
	</servlet-mapping>
</web-app>
